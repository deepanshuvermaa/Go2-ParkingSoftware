{
    "compilerOptions":  {
                            "target":  "ES2020",
                            "module":  "ESNext",
                            "moduleResolution":  "Node",
                            "lib":  [
                                        "ES2020",
                                        "DOM",
                                        "DOM.Iterable"
                                    ],
                            "jsx":  "react-native",
                            "strict":  true,
                            "allowJs":  false,
                            "skipLibCheck":  true,
                            "esModuleInterop":  true,
                            "resolveJsonModule":  true,
                            "isolatedModules":  true,
                            "noEmit":  true,
                            "types":  [
                                          "jest",
                                          "react",
                                          "react-native"
                                      ],
                            "baseUrl":  ".",
                            "paths":  {
                                          "@/assets/*":  [
                                                             "./assets/*"
                                                         ],
                                          "@/constants/*":  [
                                                                "./constants/*"
                                                            ],
                                          "@/contexts/*":  [
                                                               "./contexts/*"
                                                           ],
                                          "@/services/*":  [
                                                               "./services/*"
                                                           ],
                                          "@/stores/*":  [
                                                             "./stores/*"
                                                         ],
                                          "@/app/*":  [
                                                          "./app/*"
                                                      ],
                                          "@/hooks/*":  [
                                                            "./hooks/*"
                                                        ],
                                          "@/types/*":  [
                                                            "./types/*"
                                                        ],
                                          "@/utils/*":  [
                                                            "./utils/*"
                                                        ],
                                          "@/components/*":  [
                                                                 "./components/*"
                                                             ]
                                      }
                        },
    "extends":  "expo/tsconfig.base"
}
